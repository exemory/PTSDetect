{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "IdentityOptions": {
    "RequireUniqueEmail": true,
    "RequiredPasswordLength": 8
  },
  "JwtOptions": {
    "ClockSkew": "00:00:10",
    "Auth": {
      "Lifetime": "30:00"
    },
    "Refresh": {
      "Lifetime": "30.00:00"
    }
  },
  "AppDbCollectionNames": {
    "Users": "applicationUsers"
  }
}
